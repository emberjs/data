{
  "name": "@ember-data/store",
  "version": "5.4.0-alpha.40",
  "description": "The core of EmberData. Provides the Store service which coordinates the cache with the network and presentation layers.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:emberjs/data.git",
    "directory": "packages/store"
  },
  "license": "MIT",
  "author": "Chris Thoburn <runspired@users.noreply.github.com>",
  "files": [
    "unstable-preview-types",
    "addon-main.js",
    "addon",
    "README.md",
    "LICENSE.md",
    "ember-data-logo-dark.svg",
    "ember-data-logo-light.svg"
  ],
  "exports": {
    ".": {
      "types": "./unstable-preview-types/index.d.ts",
      "default": "./addon/index.js"
    },
    "./*": {
      "types": "./unstable-preview-types/*.d.ts",
      "default": "./addon/*.js"
    }
  },
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content --ext .js,.ts,.mjs,.cjs --report-unused-disable-directives",
    "build:runtime": "rollup --config && babel ./addon --out-dir addon --plugins=../private-build-infra/src/transforms/babel-plugin-transform-ext.js",
    "build:types": "tsc --build",
    "_build": "bun run build:runtime && bun run build:types",
    "prepack": "bun run _build",
    "_syncPnpm": "bun run sync-dependencies-meta-injected"
  },
  "dependencies": {
    "@ember-data/private-build-infra": "workspace:5.4.0-alpha.40",
    "@embroider/macros": "^1.15.0",
    "ember-cli-babel": "^8.2.0"
  },
  "dependenciesMeta": {
    "@ember-data/private-build-infra": {
      "injected": true
    },
    "@ember-data/tracking": {
      "injected": true
    },
    "@ember-data/request": {
      "injected": true
    },
    "@warp-drive/core-types": {
      "injected": true
    },
    "@ember/string": {
      "injected": true
    }
  },
  "peerDependencies": {
    "@ember-data/request": "workspace:5.4.0-alpha.40",
    "@ember-data/tracking": "workspace:5.4.0-alpha.40",
    "@ember/string": "^3.1.1",
    "@warp-drive/core-types": "workspace:0.0.0-alpha.26"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.1",
    "@babel/plugin-proposal-decorators": "^7.24.1",
    "@babel/plugin-transform-class-properties": "^7.24.1",
    "@babel/plugin-transform-private-methods": "^7.24.1",
    "@babel/plugin-transform-runtime": "^7.24.1",
    "@babel/plugin-transform-typescript": "^7.24.1",
    "@babel/preset-env": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@babel/runtime": "^7.24.1",
    "@ember-data/request": "workspace:5.4.0-alpha.40",
    "@ember-data/tracking": "workspace:5.4.0-alpha.40",
    "@ember/string": "^3.1.1",
    "@embroider/addon-dev": "^4.2.1",
    "@glimmer/component": "^1.1.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@warp-drive/core-types": "workspace:0.0.0-alpha.26",
    "@warp-drive/internal-config": "workspace:5.4.0-alpha.40",
    "ember-source": "~5.6.0",
    "pnpm-sync-dependencies-meta-injected": "0.0.10",
    "rollup": "^4.13.0",
    "typescript": "^5.4.3",
    "walk-sync": "^3.0.0",
    "webpack": "^5.91.0"
  },
  "engines": {
    "node": ">= 18.19.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "pnpm@8.15.5",
  "ember-addon": {
    "main": "addon-main.js",
    "type": "addon",
    "version": 1
  },
  "ember": {
    "edition": "octane"
  }
}
