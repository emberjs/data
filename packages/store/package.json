{
  "name": "@ember-data/store",
  "version": "5.7.0-beta.0",
  "description": "The core of EmberData. Provides the Store service which coordinates the cache with the network and presentation layers.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:emberjs/data.git",
    "directory": "packages/store"
  },
  "license": "MIT",
  "author": "Chris Thoburn <runspired@users.noreply.github.com>",
  "files": [
    "unstable-preview-types",
    "addon-main.cjs",
    "dist",
    "README.md",
    "LICENSE.md",
    "logos"
  ],
  "exports": {
    ".": {
      "types": "./unstable-preview-types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./unstable-preview-types/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "build:pkg": "vite build;",
    "prepack": "pnpm run build:pkg",
    "sync": "echo \"syncing\"",
    "start": "vite"
  },
  "dependencies": {
    "@embroider/macros": "^1.18.1",
    "@warp-drive/core": "workspace:*"
  },
  "peerDependencies": {
    "@ember-data/tracking": "workspace:*",
    "@ember/test-waiters": "^3.1.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@ember-data/tracking": {
      "optional": true
    },
    "@ember/test-waiters": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@warp-drive/internal-config": "workspace:*",
    "@ember-data/tracking": "workspace:*",
    "@ember/test-waiters": "^4.1.1",
    "ember-source": "~6.6.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "pnpm@10.15.0",
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "ember": {
    "edition": "octane"
  }
}
