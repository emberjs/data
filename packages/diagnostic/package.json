{
  "name": "@warp-drive/diagnostic",
  "version": "0.0.3-alpha.98",
  "private": true,
  "description": "⚡️ A Lightweight Modern Test Runner",
  "keywords": [
    "test",
    "assert",
    "testrunner",
    "tap",
    "reporter",
    "junit",
    "qunit",
    "vitest",
    "jest",
    "mocha",
    "chai",
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:warp-drive-data/warp-drive.git",
    "directory": "packages/diagnostic"
  },
  "license": "MIT",
  "author": "Chris Thoburn <runspired@users.noreply.github.com>",
  "files": [
    "unstable-preview-types",
    "addon-main.cjs",
    "dist/",
    "server/",
    "README.md",
    "LICENSE.md",
    "logos"
  ],
  "exports": {
    "./server": {
      "node": "./server/index.js",
      "bun": "./server/index.js",
      "deno": "./server/index.js",
      "default": "./server/index.js"
    },
    ".": {
      "types": "./unstable-preview-types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*.css": {
      "default": "./dist/*.css"
    },
    "./server/*": {
      "node": "./dist-server/*.js",
      "bun": "./dist-server/*.js",
      "deno": "./dist-server/*.js"
    },
    "./ember": {
      "types": "./unstable-preview-types/ember.d.ts",
      "default": "./dist/ember.js"
    },
    "./*": {
      "types": "./unstable-preview-types/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "build:tests": "rm -rf dist-test && cp -R test dist-test && mkdir -p dist-test/@warp-drive && cp -R dist dist-test/@warp-drive/diagnostic",
    "build:pkg": "vite build; vite build -c ./vite.config-server.mjs",
    "prepack": "pnpm run build:pkg",
    "sync": "echo \"syncing\"",
    "start": "vite"
  },
  "peerDependencies": {
    "@ember/test-helpers": "5.2.0",
    "ember-cli-test-loader": ">= 3.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "peerDependenciesMeta": {
    "@ember/test-helpers": {
      "optional": true
    },
    "ember-cli-test-loader": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "debug": "^4.4.1",
    "tmp": "^0.2.5",
    "@warp-drive/core": "workspace:*",
    "@warp-drive/react": "workspace:*",
    "@embroider/macros": "^1.18.1",
    "dom-element-descriptors": "^0.5.1",
    "hono": "^4.9.4",
    "@hono/node-server": "^1.19.0",
    "@hono/node-ws": "^1.2.0",
    "qunit-dom": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.28.3",
    "@warp-drive/internal-config": "workspace:*",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@types/node": "^24.3.0",
    "@types/tmp": "^0.2.6",
    "@types/debug": "^4.1.12",
    "@ember/test-helpers": "5.2.0",
    "ember-source": "~6.6.0",
    "@glimmer/component": "^2.0.0",
    "ember-cli-test-loader": "^3.1.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "pnpm@10.15.0",
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "ember": {
    "edition": "octane"
  }
}
