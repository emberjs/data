    "@embroider/macros": "^1.16.10",
    "@warp-drive/build-config": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-typescript": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
