{
  "name": "@ember-data/unpublished-test-infra",
  "version": "5.8.0-alpha.2",
  "private": true,
  "description": "The default blueprint for ember-data private packages.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/warp-drive-data/warp-drive.git",
    "directory": "packages/unpublished-test-infra"
  },
  "license": "MIT",
  "author": "",
  "files": [
    "unstable-preview-types",
    "addon-main.cjs",
    "testem",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./unstable-preview-types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./testem/*": {
      "default": "./testem/*.js"
    },
    "./*": {
      "types": "./unstable-preview-types/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build:pkg": "vite build;",
    "prepack": "pnpm run build:pkg",
    "sync": "echo \"syncing\""
  },
  "peerDependencies": {
    "qunit": "^2.20.1",
    "testem": "^3.12.0",
    "@ember-data/store": "workspace:*",
    "@warp-drive/diagnostic": "workspace:*",
    "@warp-drive/core-types": "workspace:*",
    "@ember/test-helpers": "3.3.0 || ^4.0.4 || ^5.1.0"
  },
  "peerDependenciesMeta": {
    "qunit": {
      "optional": true
    },
    "testem": {
      "optional": true
    },
    "@warp-drive/diagnostic": {
      "optional": true
    }
  },
  "dependencies": {
    "@embroider/macros": "^1.18.1",
    "chalk": "^4.1.2",
    "semver": "^7.7.2",
    "@warp-drive/build-config": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@ember/test-helpers": "5.2.0",
    "@glimmer/component": "^2.0.0",
    "@types/semver": "^7.7.0",
    "@types/qunit": "2.19.10",
    "@ember-data/request": "workspace:*",
    "@ember-data/request-utils": "workspace:*",
    "@ember-data/store": "workspace:*",
    "@warp-drive/core-types": "workspace:*",
    "@warp-drive/diagnostic": "workspace:*",
    "@warp-drive/internal-config": "workspace:*",
    "ember-source": "~6.6.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "qunit": "^2.20.1",
    "testem": "^3.12.0"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "pnpm@10.15.0"
}
