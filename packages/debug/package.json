{
  "name": "@ember-data/debug",
  "version": "5.8.0-alpha.0",
  "description": "Provides support for the ember-inspector for apps built with Ember and EmberData",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:warp-drive-data/warp-drive.git",
    "directory": "packages/debug"
  },
  "license": "MIT",
  "author": "",
  "directories": {},
  "scripts": {
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "build:pkg": "vite build;",
    "prepack": "pnpm run build:pkg",
    "sync": "echo \"syncing\"",
    "start": "vite"
  },
  "files": [
    "unstable-preview-types",
    "addon-main.cjs",
    "dist",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md",
    "logos"
  ],
  "exports": {
    ".": {
      "types": "./unstable-preview-types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./unstable-preview-types/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "peerDependencies": {
    "@ember-data/store": "workspace:*",
    "@ember-data/model": "workspace:*",
    "@ember-data/request-utils": "workspace:*",
    "@warp-drive/core-types": "workspace:*"
  },
  "dependencies": {
    "@ember/edition-utils": "^1.2.0",
    "@embroider/macros": "^1.18.1",
    "@warp-drive/core": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@ember-data/request": "workspace:*",
    "@ember-data/legacy-compat": "workspace:*",
    "@ember-data/request-utils": "workspace:*",
    "@ember-data/store": "workspace:*",
    "@ember-data/model": "workspace:*",
    "@warp-drive/core-types": "workspace:*",
    "@warp-drive/internal-config": "workspace:*",
    "ember-source": "~6.6.0",
    "decorator-transforms": "^2.3.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  },
  "ember-addon": {
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2,
    "app-js": {
      "./data-adapter.js": "./dist/_app_/data-adapter.js"
    }
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "pnpm@10.15.0"
}
