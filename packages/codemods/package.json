{
  "name": "@ember-data/codemods",
  "private": true,
  "version": "5.4.0-alpha.52",
  "description": "Codemods for EmberData paradigms",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:emberjs/data.git",
    "directory": "packages/codemods"
  },
  "bin": {
    "codemods": "codemods"
  },
  "scripts": {
    "build:cli": "bun build bin/codemods.ts --compile --outfile codemods",
    "_build": "bun run build:cli",
    "prepack": "bun run _build",
    "check:types": "tsc --noEmit",
    "lint": "eslint . --quiet --cache --cache-strategy=content --ext .js,.ts,.mjs,.cjs --report-unused-disable-directives",
    "_syncPnpm": "bun run sync-dependencies-meta-injected"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "files": [
    "bin",
    "src"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "prettier": "^3.2.5"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^9.2.0",
    "jscodeshift": "^0.15.2",
    "typescript": "^5.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jscodeshift": "0.11.11",
    "@types/bun": "^1.1.0",
    "@warp-drive/internal-config": "workspace:5.4.0-alpha.53",
    "eslint": "^8.57.0",
    "pnpm-sync-dependencies-meta-injected": "0.0.10",
    "prettier": "^3.2.5",
    "qunit": "^2.20.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup": "^4.14.1"
  },
  "engines": {
    "node": ">= 18.20.1"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "packageManager": "pnpm@8.15.6"
}
