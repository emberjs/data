{
  "name": "eslint-plugin-warp-drive",
  "description": "ESLint rules for Applications using WarpDrive or EmberData",
  "main": "./src/index.js",
  "version": "5.8.0-alpha.17",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/warp-drive-data/warp-drive.git",
    "directory": "packages/eslint-plugin-warp-drive"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "exports": {
    ".": {
      "default": "./src/index.js"
    },
    "./recommended": {
      "default": "./src/recommended.js"
    }
  },
  "dependencies": {
    "requireindex": "^1.2.0",
    "@warp-drive/core": "workspace:*",
    "@warp-drive/utilities": "workspace:*"
  },
  "devDependencies": {
    "@warp-drive/internal-config": "workspace:*",
    "@babel/plugin-proposal-decorators": "7.28.0",
    "@babel/core": "^7.28.3",
    "eslint": "^9.34.0",
    "mocha": "^11.1.0"
  },
  "files": [
    "src",
    "docs",
    "CHANGELOG.md",
    "README.md",
    "logos",
    "LICENSE.md"
  ],
  "scripts": {
    "test": "mocha tests",
    "lint": "eslint . --quiet --cache --cache-strategy=content"
  },
  "license": "MIT"
}
