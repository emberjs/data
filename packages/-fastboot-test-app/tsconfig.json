{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "noEmit": true,
    "skipLibCheck": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "module": "esnext",
    "paths": {
      "fastboot-test-app/tests/*": ["tests/*"],
      "fastboot-test-app/*": ["app/*"],
      "ember-data": ["../-ember-data/addon"],
      "ember-data/*": ["../-ember-data/addon/*"],
      "@ember-data": ["../-ember-data/addon"],
      "@ember-data/*": ["../-ember-data/addon/*"],
      "@ember-data/store": ["../store/addon"],
      "@ember-data/store/*": ["../store/addon/*"],
      "@ember-data/model": ["../model/addon"],
      "@ember-data/model/*": ["../model/addon/*"],
      "@ember-data/adapter": ["../adapter/addon"],
      "@ember-data/adapter/*": ["../adapter/addon/*"],
      "@ember-data/serializer": ["../serializer/addon"],
      "@ember-data/serializer/*": ["../serializer/addon/*"],
      "ember-data/test-support": ["../-ember-data/addon-test-support"],
      "ember-data/test-support/*": ["../-ember-data/addon-test-support/*"],
      "@ember-data/adapter/error": ["../adapter/addon/error"],
      "@ember-data/canary-features": ["../canary-features/addon"],
      "*": [
        "../store/types/*",
        "types/*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "tests/**/*",
    "types/**/*"
  ]
}
