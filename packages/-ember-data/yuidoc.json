{
  "name": "The ember-data API",
  "description": "The ember-data API: a data persistence library for Ember.js",
  "url": "https://github.com/emberjs/data",
  "options": {
    "enabledEnvironments": [],
    "extension": ".js,.ts",
    "preprocessor": "./lib/yui-docs-preprocessor.js",
    "excludeTags": [
      "internal",
      "feature"
    ],
    "paths": [
      "addon",
      "../adapter/addon",
      "../model/addon",
      "../serializer/addon",
      "../store/addon",
      "../record-data/addon",
      "../debug/addon",
      "../private-build-infra/addon",
      "../canary-features/addon"
    ],
    "exclude": "vendor",
    "outdir": "dist/docs"
  }
}