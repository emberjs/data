{
  "name": "The ember-data API",
  "description": "The ember-data API: a data persistence library for Ember.js",
  "url": "https://github.com/emberjs/data",
  "options": {
    "enabledEnvironments": [],
    "extension": ".js,.ts",
    "paths": [
      "addon",
      "../../node_modules/ember-inflector/addon",
      "../adapter/addon",
      "../model/addon",
      "../serializer/addon",
      "../store/addon",
      "../record-data/addon",
      "../debug/addon",
      "../private-build-infra/addon",
      "../canary-features/addon"
    ],
    "exclude": "vendor",
    "outdir": "dist/docs"
  }
}
