{
  "name": "The ember-data API",
  "description": "The ember-data API: a data persistence library for Ember.js",
  "url": "https://github.com/emberjs/data",
  "options": {
    "enabledEnvironments": ["production"],
    "extension": ".js,.ts",
    "paths": [
      "addon",
      "../../node_modules/ember-inflector/addon",
      "../adapter/addon",
      "../model/addon",
      "../serializer/addon",
      "../store/addon",
      "../store/addon/-private/system"
    ],
    "exclude": "vendor",
    "outdir": "docs"
  }
}
