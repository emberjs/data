  },
  "peerDependenciesMeta": {},
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.28.3",
    "decorator-transforms": "^2.3.0",
