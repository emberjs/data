{
  "name": "@warp-drive/internal-docs-viewer",
  "version": "5.7.0-alpha.7",
  "description": "API Docs Viewer for the WarpDrive Project Monorepo | Unpublished",
  "private": true,
  "type": "module",
  "files": [
    "src"
  ],
  "bin": {
    "preview-api-docs": "./src/preview-ember-docs.ts",
