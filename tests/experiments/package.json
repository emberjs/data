{
  "name": "experiments",
  "version": "5.8.0-alpha.2",
  "description": "Provides tests for @warp-drive/experiments",
  "private": true,
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:warp-drive-data/warp-drive.git",
    "directory": "tests/experiments"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build:tests": "export IS_TESTING=true; export NODE_ENV=development; vite build -m development --outDir dist-test",
    "build:production": "export IS_TESTING=true; export NODE_ENV=production; vite build --outDir dist-test",
    "lint": "eslint . --quiet --cache --cache-strategy=content",
    "check:types": "glint --noEmit",
    "start": "export IS_TESTING=true; export NODE_ENV=development; vite build -m development --watch --outDir dist-test",
    "test": "export IS_TESTING=true; export NODE_ENV=development; bun ./diagnostic.js",
    "test:production": "export IS_TESTING=true; export NODE_ENV=production; bun ./diagnostic.js",
    "test:start": "bun ./diagnostic.js --serve --no-launch"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-transform-typescript": "^7.28.0",
    "@babel/runtime": "^7.28.3",
    "@ember-data/debug": "workspace:*",
    "@ember-data/graph": "workspace:*",
    "@ember-data/json-api": "workspace:*",
    "@ember-data/legacy-compat": "workspace:*",
    "@ember-data/model": "workspace:*",
    "@ember-data/request-utils": "workspace:*",
    "@ember-data/request": "workspace:*",
    "@ember-data/store": "workspace:*",
    "@ember-data/unpublished-test-infra": "workspace:*",
    "@ember/test-helpers": "5.2.0",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/core": "^4.1.3",
    "@embroider/macros": "^1.18.1",
    "@embroider/vite": "^1.2.0",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "alpha",
    "@glint/environment-ember-loose": "alpha",
    "@glint/environment-ember-template-imports": "alpha",
    "@glint/template": "alpha",
    "@glint/tsserver-plugin": "alpha",
    "@rollup/plugin-babel": "^6.0.4",
    "@warp-drive/build-config": "workspace:*",
    "@warp-drive/core-types": "workspace:*",
    "@warp-drive/core": "workspace:*",
    "@warp-drive/diagnostic": "workspace:*",
    "@warp-drive/ember": "workspace:*",
    "@warp-drive/experiments": "workspace:*",
    "@warp-drive/holodeck": "workspace:*",
    "@warp-drive/internal-config": "workspace:*",
    "@warp-drive/schema-record": "workspace:*",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "decorator-transforms": "^2.3.0",
    "ember-source": "^6.6.0",
    "ember-strict-application-resolver": "^0.1.0",
    "rollup": "^4.48.0",
    "terser": "^5.43.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
